#include <iostream>
using namespace std;

template < typename T >
void LinkedList<T>::printList(const LinkedList<T>::Node *n, std::ostream &os) const {
  if(!n)
    return;
  else {
    
    os << n->value << endl;
    
    if(n->next)
      printList(n->next, os);
 

  }
}

template < typename T >
void LinkedList<T>::deleteList(LinkedList<T>::Node *n){

  if(n == NULL)
    return;

  deleteList(n -> next)

  delete n;
  return;
}
